<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="beans.xsd">

	<bean class="de.hybris.platform.commercefacades.product.data.ProductData">
		<property name="questions" type="java.util.List&lt;com.myapp.questions.data.QuestionData&gt;" />
	</bean>

	<bean class="com.myapp.questions.data.QuestionData">
		<property name="question" type="String"/>
		<property name="answer" type="String"/>
		<property name="questionCustomer" type="String"/>
		<property name="answerCustomer" type="String"/>
	</bean>

	<enum class="de.hybris.platform.commercefacades.product.ProductOption">
		<value>QUESTIONS</value>
		<value>WARRANTY</value>
	</enum>

</beans>
